
<%- include("hedder") -%>

<div class="box" id="heading">
  <h1><%= listtitle %> </h1>
</div>

<div class="box">
  <% for (var i = 0;i< newitem.length ;i++){ %>
  <div class="item">
    <input type="checkbox">
    <p> <%= newitem[i] %> </p>
  </div>
  <% } %>

  <form action="/" method="post">
    <input type="text" name="try" placeholder="New item" autocomplete="off">
    <button type="submit" name="list" value=<%= listtitle %>>+</button>
  </form>
</div>

<%- include("fotter") -%>
